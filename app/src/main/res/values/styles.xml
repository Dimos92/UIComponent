<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="VolumeProgress">
        <item name="android:progressDrawable">@drawable/volume_drawable</item>
        <item name="android:indeterminateDrawable">@drawable/indeterminate_drawable</item>
        <item name="android:thumb">@drawable/volume_thumb</item>
        <item name="android:minHeight">9.3dp</item>
        <item name="android:maxHeight">9.3dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:thumbOffset">0dp</item>
        <item name="android:focusable">true</item>
        <item name="android:splitTrack">false</item>
    </style>


</resources>
